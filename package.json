{
  "name": "pohina-app",
  "version": "0.0.1",
  "description": "pohiseva backend",
  "main": "app.js",
  "engines": {
    "node": "5.9.0"
  },
  "scripts": {
    "lint": "eslint ./**/*.js || true",
    "lint:fix": "eslint ./**/*.js --fix",
    "start": "node ./scripts/check_prod.js && node app.js || nodemon app.js",
    "pm2:start": "pm2 start -n myapp-api app.js",
    "db": "node ./scripts/db.js",
    "test": "istanbul cover node_modules/mocha/bin/_mocha --require test/testhelper.js test/**/*.js",
    "test:noc": "mocha --require test/testhelper.js test/**/*.js",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/team-pohina-elohack16/pohina-backend.git"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.0",
    "connect-busboy": "0.0.2",
    "cors": "^2.7.1",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "jwt-simple": "^0.5.0",
    "lodash": "^4.15.0",
    "moment": "^2.14.1",
    "mongoose": "^4.5.8",
    "morgan": "^1.7.0",
    "schema-inspector": "^1.6.5"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.6.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.2.3",
    "istanbul": "^0.4.3",
    "mocha": "2.4.5",
    "nock": "^8.0.0",
    "nodemon": "^1.9.1",
    "sinon": "^1.17.3",
    "supertest": "^1.2.0"
  }
}
